{% extends 'AppBundle:default:base.html.twig' %}

{% block body %}
    <h1>Achat</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ achat.id }}</td>
            </tr>
            <tr>
                <th>Dateachat</th>
                <td>{% if achat.dateAchat %}{{ achat.dateAchat|date('Y-m-d') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('business_achat_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('business_achat_edit', { 'id': achat.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
