{% extends 'AppBundle:default:base.html.twig' %}

{% block body %}
    <h1>Prix</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ prix.id }}</td>
            </tr>
            <tr>
                <th>Dateassignation</th>
                <td>{% if prix.dateAssignation %}{{ prix.dateAssignation|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Montant</th>
                <td>{{ prix.montant }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('business_prix_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('business_prix_edit', { 'id': prix.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
